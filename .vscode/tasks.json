{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "build-debug-linux",
        "type": "shell",
        "command": "cmake -B build -DCMAKE_BUILD_TYPE=Debug -DUSE_NATIVE_ARCH=1 && cmake --build build -- -j $(nproc)",
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "options": {
          "cwd": "${workspaceFolder}"
        }
      },
      {
        "label": "build-release-linux",
        "type": "shell",
        "command": "cmake -B build -DUSE_NATIVE_ARCH=1 && cmake --build build -- -j $(nproc)",
        "group": {
          "kind": "build",
          "isDefault": false
        },
        "options": {
          "cwd": "${workspaceFolder}"
        }
    },
    {
      "label": "run-release",
      "type": "shell",
      "command": "cmake -B build -DUSE_NATIVE_ARCH=1 && cmake --build build -- -j $(nproc) && ./build/NEURAL_AMP_MODELER_artefacts/Release/Standalone/Neural\\ Amp\\ Modeler ",
      "group": {
        "kind": "build",
        "isDefault": false
      },
      "options": {
        "cwd": "${workspaceFolder}"
      }
    }
  ]
}